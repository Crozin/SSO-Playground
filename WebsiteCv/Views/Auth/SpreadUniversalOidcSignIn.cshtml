@model IList<SharedNet.UniversalSignInCodeDto>

@foreach (var usic in Model)
{
    <script>
        (function() {
            "use strict";

            var xhr = new XMLHttpRequest();

            xhr.open("POST", "@usic.TargetUri", true);
            xhr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
            xhr.withCredentials = true;
            xhr.send("code=" + encodeURIComponent("@usic.Code"));
        })();
    </script>
}